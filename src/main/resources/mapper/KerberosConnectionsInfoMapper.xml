<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.thriteen.bgd.usersync.dao.dbmeta.KerberosConnectionsInfoMapper">


    <select id="selectByConnectName" resultType="java.lang.Integer">
        select count(*) num from kerberos_connections_info where kerberos_connect_name=#{kerberosConnectName}
    </select>
</mapper>